{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Number of Phone Subscribers in India",
    "anchor": "start",
    "subtitle": "Millions",
    "fontSize": "14",
    "subtitleFontSize": "12",
    "color": "#000000",
    "subtitleColor": "#000000"
  },
  "height": "250",
  "width": "350",
  "autosize": {"type": "fit", "contains": "padding"},
  "background": "#ffffff",
  "view": {"fill": "#ffffff", "strokeOpacity": 0},
  "data": {
    "url": "https://api.economicsobservatory.com/ind/tele?vega",
    "format": {"type": "json"}
  },
  "mark": {"type": "area", "color": "#4FB286", "strokeWidth": "2"},
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "axis": {"title": "", "titleColor": "#122B39", "grid": false}
    }
  },
  "config": {}
}